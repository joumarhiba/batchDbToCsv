package fr.cnamts.ex.batch.mapping;

import java.sql.ResultSet;
import java.sql.SQLException;

import org.springframework.jdbc.core.RowMapper;

import fr.cnamts.ex.batch.bo.EmployeeDto;


public class EmployeeRowMapping implements RowMapper<EmployeeDto>{

	@Override
	public EmployeeDto mapRow(ResultSet rs, int nbRow) throws SQLException {

		EmployeeDto employeeDto = new EmployeeDto();
		employeeDto.setName(rs.getString("name"));  
		employeeDto.setPhone(rs.getString("phone"));
		employeeDto.setEmail(rs.getString("email"));
		employeeDto.setCountry(rs.getString("country"));  


		return employeeDto;
	}

}
